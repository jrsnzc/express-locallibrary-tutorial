{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "mocha": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off",
        "no-use-before-define": ["error", { "functions": false }]
    }
}