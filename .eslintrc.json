{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "func-names": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "off",
        "no-use-before-define": ["error", { "functions": false }],
        "indent": "off",
        "prefer-arrow-callback": "off"
    }
}
